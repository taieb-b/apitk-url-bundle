{
  "name": "check24/apitk-url-bundle",
  "version": "2.0.0",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "This bundle provides filter, sorting and pagination for RESTful API's",
  "authors": [
    {
      "name": "Michael Wolf",
      "email": "wulf1337@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shopping\\ApiTKUrlBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/byWulf/apitk-common-bundle.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/byWulf/apitk-header-bundle.git"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/config": ">=4.3 <6.0",
    "symfony/dependency-injection": ">=4.3 <6.0",
    "symfony/http-kernel": ">=4.3 <6.0",
    "symfony/framework-bundle": ">=4.3 <6.0",
    "doctrine/annotations": "^1.8",
    "nelmio/api-doc-bundle": "^3.4",
    "sensio/framework-extra-bundle": "^5.1",
    "check24/apitk-common-bundle": "^2.0",
    "check24/apitk-header-bundle": "^2.0"
  },
  "require-dev": {
    "captainhook/captainhook": "^5.0",
    "captainhook/plugin-composer": "^5.1",
    "doctrine/orm": "^2.6",
    "doctrine/doctrine-bundle": ">=1.8 <3.0",
    "friendsofphp/php-cs-fixer": "^2.13",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11.19"
  }
}
